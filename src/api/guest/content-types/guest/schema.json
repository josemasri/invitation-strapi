{
  "kind": "collectionType",
  "collectionName": "guests",
  "info": {
    "singularName": "guest",
    "pluralName": "guests",
    "displayName": "Guest",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "invitationName": {
      "type": "string"
    },
    "maxGuests": {
      "type": "integer",
      "default": 1
    },
    "countryCode": {
      "type": "string",
      "default": "521",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "confirmed": {
      "type": "enumeration",
      "enum": [
        "unknown",
        "yes",
        "no"
      ],
      "default": "unknown"
    },
    "confirmedGuests": {
      "type": "integer"
    },
    "invitedBy": {
      "type": "enumeration",
      "enum": [
        "Groom",
        "Bride"
      ]
    },
    "timesSended": {
      "type": "integer",
      "default": 0
    },
    "event": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event.event",
      "inversedBy": "guests"
    },
    "eventConfirmations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event-confirmation.event-confirmation",
      "mappedBy": "guest"
    },
    "messageStatuses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::message-status.message-status",
      "mappedBy": "guest"
    }
  }
}
